<?xml version="1.0" encoding="UTF-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Stop Playing Sounds in the Background </Title>
      <Author>Microsoft Corporation</Author>
      <Description>Stops a sound playing in the background.</Description>
      <Shortcut>sndStop</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>System</Namespace>
        </Import>
        <Import>
          <Namespace>Microsoft.VisualBasic</Namespace>
        </Import>
      </Imports>
      <Code Language="VB" Kind="method body"><![CDATA[My.Computer.Audio.Stop()]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>